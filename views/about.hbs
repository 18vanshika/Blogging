    <section id="contact" class="py-5  background1">
    <h2>Subscribe for More</h2>
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="row">
            <form action="/process-contact-form" method="post">
              <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label"><b>Drop your Email to Subscribe</b></label>
                <input name="email" type="email" class="form-control" id="exampleInputEmail1"
                  aria-describedby="emailHelp">
              </div>
              <div class="mb-3">
                <label for="Description" class="form-label"><b>Send me your feedback</b></label>
                <textarea name="query" type="floatingTextarea" class="form-control" id="exampleInputdescription"
                  aria-describedby="describe"></textarea>
              </div>
              <button type="submit" class="btn btn-primary">Send</button>
            </form>
          </div>
        </div>
      </div>
    </div>
